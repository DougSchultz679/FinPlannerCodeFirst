<!DOCTYPE html>
<html>
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Meta -->
    <meta name="description" content="Financial Planner">
    <meta name="author" content="Douglas Schultz">

    <title>Financial Planner App</title>

    <!-- vendor css -->
    <link href="~/Assets/Template/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Assets/Template/lib/Ionicons/css/ionicons.css" rel="stylesheet" />
    <link href="~/Assets/Template/lib/chartist/css/chartist.css" rel="stylesheet" />
    <link href="~/Assets/Template/lib/rickshaw/css/rickshaw.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" />

    <!-- Slim CSS -->
    <link href="~/Assets/Template/css/slim.css" rel="stylesheet" />

</head>
<body>
    <div class="slim-header">
        <div class="container">
            <div class="slim-header-left">
                <h2 class="slim-logo">
                    @*FIX THIS LINK*@
                    <a href="@Url.Action("Index")">
                        $Planner
                    </a>
                </h2>
            </div>
            <div class="slim-header-right">
                <div class="dropdown dropdown-a">
                    <a href="" class="header-notification" data-toggle="dropdown">
                        <i class="icon ion-ios-bolt-outline"></i>
                    </a>
                    <div class="dropdown-menu">
                        <div class="dropdown-menu-header">
                            <h6 class="dropdown-menu-title">Navigation Links</h6>
                            <div>
                            </div>
                        </div>
                        @*FIX THIS - LINKS*@
                        <div class="dropdown-activity-list">
                            <div class="activity-label">Jump to:</div>
                            <div class="activity-item">
                                <div class="row no-gutters">
                                    <div class="col-2 tx-right"></div>
                                    <div class="col-8">Nav Item</div>
                                </div><!-- row -->
                            </div>
                            <div class="activity-item">
                                <div class="row no-gutters">
                                    <div class="col-2 tx-right"></div>
                                    <div class="col-8">Nav Item</div>
                                </div><!-- row -->
                            </div>
                            <div class="activity-item">
                                <div class="row no-gutters">
                                    <div class="col-2 tx-right"></div>
                                    <div class="col-8">Nav Item</div>
                                </div><!-- row -->
                            </div>
                        </div><!-- dropdown-activity-list -->
                    </div><!-- dropdown-menu-right -->
                </div><!-- dropdown -->
                <div class="dropdown dropdown-c">
                    <a href="#" class="logged-user" data-toggle="dropdown">
                        <img src="http://via.placeholder.com/500x500" alt="">
                        <span>Katherine</span>
                        <i class="fa fa-angle-down"></i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right">
                        <nav class="nav">
                            <a href="page-profile.html" class="nav-link"><i class="icon ion-person"></i> View Profile</a>
                            <a href="page-edit-profile.html" class="nav-link"><i class="icon ion-compose"></i> Edit Profile</a>
                            <a href="page-activity.html" class="nav-link"><i class="icon ion-ios-bolt"></i> Activity Log</a>
                            <a href="page-settings.html" class="nav-link"><i class="icon ion-ios-gear"></i> Account Settings</a>
                            <a href="page-signin.html" class="nav-link"><i class="icon ion-forward"></i> Sign Out</a>
                        </nav>
                    </div><!-- dropdown-menu -->
                </div>
                <div class="pull-right">
                    @Html.Partial("_LoginPartial")
                </div>
                <!-- dropdown --><!-- dropdown-menu-right -->
                @*<ul class="">
                    FIX THIS - these links all need to go into the nav bar.
                                    @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })

                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    </ul>*@
            </div>
        </div>
    </div>


    <div class="slim-mainpanel">
        @RenderBody()
    </div>

    <div class="slim-footer">
        <div class="container">
            <p>Copyright 2018 &copy; All Rights Reserved. Slim Dashboard Template</p>
            <p>Designed by: <a href="">ThemePixels</a></p>
        </div><!-- container -->
    </div><!-- slim-footer -->


    <script src="~/Assets/Template/lib/jquery/js/jquery.js"></script>
    <script src="~/Assets/Template/lib/popper.js/js/popper.js"></script>
    <script src="~/Assets/Template/lib/bootstrap/js/bootstrap.js"></script>
    <script src="~/Assets/Template/lib/jquery.cookie/js/jquery.cookie.js"></script>
    <script src="~/Assets/Template/lib/chartist/js/chartist.js"></script>
    <script src="~/Assets/Template/lib/d3/js/d3.js"></script>
    <script src="~/Assets/Template/lib/rickshaw/js/rickshaw.min.js"></script>
    <script src="~/Assets/Template/lib/jquery.sparkline.bower/js/jquery.sparkline.min.js"></script>
    <script src="~/Assets/Template/js/ResizeSensor.js"></script>
    <script src="~/Assets/Template/js/dashboard.js"></script>
    <script src="~/Assets/Template/js/slim.js"></script>

    <!--FIX THIS-->
    @RenderSection("scripts", required: false)
</body>
</html>
